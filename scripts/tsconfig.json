{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "TenantFlow Scripts TypeScript Config",
	"extends": "../packages/typescript-config/base.json",
	"compilerOptions": {
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"target": "ES2022",
		"noEmit": true,
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"include": ["./**/*.ts"]
}
