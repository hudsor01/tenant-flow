{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "remoteCache": {
    "enabled": false
  },
  "globalEnv": [
    "NODE_ENV",
    "PORT",
    "VERCEL_URL",
    "CI",
    "VERCEL",
    "VERCEL_ENV"
  ],
  "globalDependencies": [
    "**/.env.*"
  ],
  "envMode": "strict",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!**/*.test.*",
        "!**/*.spec.*",
        "!**/*.stories.*"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**",
        ".turbo/**"
      ],
      "env": [
        "NODE_ENV",
        "VITE_*",
        "DATABASE_URL",
        "JWT_SECRET",
        "SUPABASE_*",
        "STRIPE_*",
        "GOOGLE_*",
        "RESEND_*"
      ],
      "passThroughEnv": [
        "AWS_*",
        "VERCEL_*"
      ]
    },
    "backend#build": {
      "dependsOn": ["^build", "generate"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!**/*.test.*",
        "!**/*.spec.*",
        "!**/*.stories.*"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".turbo/**"
      ],
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "JWT_SECRET",
        "SUPABASE_*",
        "STRIPE_*",
        "GOOGLE_*",
        "RESEND_*"
      ],
      "passThroughEnv": [
        "AWS_*",
        "VERCEL_*"
      ]
    },
    "@tenantflow/frontend#build": {
      "dependsOn": ["^build", "@tenantflow/shared#build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!**/*.test.*",
        "!**/*.spec.*",
        "!**/*.stories.*"
      ],
      "outputs": [
        "dist/**",
        ".turbo/**"
      ],
      "env": [
        "NODE_ENV",
        "VITE_*"
      ],
      "passThroughEnv": [
        "AWS_*",
        "VERCEL_*"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^db:generate"],
      "env": [
        "NODE_ENV",
        "VITE_*",
        "DATABASE_URL",
        "JWT_SECRET",
        "SUPABASE_*",
        "STRIPE_*",
        "GOOGLE_*",
        "RESEND_*",
        "PORT"
      ]
    },
    "topo": {
      "dependsOn": ["^topo"]
    },
    "check-types": {
      "dependsOn": ["topo"],
      "outputs": [],
      "cache": true
    },
    "generate": {
      "cache": false,
      "inputs": [
        "prisma/schema.prisma",
        "package.json"
      ],
      "outputs": [
        "node_modules/.prisma/**",
        "node_modules/@prisma/**",
        "src/generated/**"
      ],
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:generate": {
      "cache": false,
      "inputs": [
        "prisma/schema.prisma",
        "package.json"
      ],
      "outputs": [
        "node_modules/.prisma/**",
        "node_modules/@prisma/**"
      ],
      "env": [
        "DATABASE_URL"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".eslintrc*",
        "eslint.config.*"
      ],
      "outputs": [],
      "cache": true
    },
    "lint:fix": {
      "dependsOn": ["^lint"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".eslintrc*",
        "eslint.config.*"
      ],
      "outputs": [],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tsconfig*.json"
      ],
      "outputs": [".tsbuildinfo"],
      "cache": true
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "test/**/*",
        "tests/**/*",
        "jest.config.*",
        "vitest.config.*",
        "playwright.config.*"
      ],
      "outputs": [
        "coverage/**",
        "test-results/**",
        ".nyc_output/**"
      ],
      "env": [
        "NODE_ENV"
      ]
    },
    "test:unit": {
      "dependsOn": ["generate"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "jest.config.*"
      ],
      "outputs": [
        "coverage/**",
        ".nyc_output/**"
      ],
      "env": [
        "NODE_ENV"
      ]
    },
    "test:e2e": {
      "dependsOn": ["build"],
      "cache": false,
      "inputs": [
        "tests/**/*",
        "e2e/**/*",
        "playwright.config.*",
        ".env.test"
      ],
      "outputs": [
        "test-results/**",
        "playwright-report/**"
      ],
      "env": [
        "TEST_DATABASE_URL",
        "PLAYWRIGHT_BASE_URL",
        "NODE_ENV"
      ]
    },
    "test:e2e:headed": {
      "dependsOn": ["build"],
      "cache": false,
      "inputs": [
        "tests/**/*",
        "playwright.config.*"
      ]
    },
    "test:e2e:ui": {
      "dependsOn": ["build"],
      "cache": false,
      "inputs": [
        "tests/**/*",
        "playwright.config.*"
      ]
    },
    "test:all": {
      "dependsOn": ["test:unit", "test:e2e"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "prisma:studio": {
      "cache": false,
      "persistent": true,
      "env": [
        "DATABASE_URL"
      ]
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "format:check": {
      "outputs": [],
      "cache": true
    },
    "backend#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["generate"],
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "JWT_SECRET",
        "SUPABASE_*",
        "STRIPE_*",
        "GOOGLE_*",
        "RESEND_*",
        "PORT"
      ]
    },
    "backend#typecheck": {
      "dependsOn": ["generate"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tsconfig*.json"
      ],
      "outputs": [],
      "cache": false
    },
    "backend#generate": {
      "cache": true,
      "inputs": [
        "prisma/schema.prisma",
        "package.json",
        ".env"
      ],
      "outputs": [
        "../../node_modules/.prisma/**",
        "../../node_modules/@prisma/client/**"
      ],
      "env": [
        "DATABASE_URL"
      ]
    },
    "shared#build": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!**/*.test.*",
        "!**/*.spec.*",
        "!**/*.stories.*"
      ],
      "outputs": [
        "dist/**",
        ".turbo/**"
      ]
    },
    "shared#typecheck": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tsconfig*.json"
      ],
      "outputs": [],
      "cache": false
    }
  }
}
