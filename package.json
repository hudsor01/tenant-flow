{
	"$schema": "https://json.schemastore.org/package",
	"name": "tenantflow",
	"private": true,
	"version": "1.0.1",
	"type": "module",
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=9"
	},
	"os": [
		"darwin",
		"linux",
		"win32"
	],
	"cpu": [
		"arm64",
		"x64"
	],
	"packageManager": "pnpm@10.18.3",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"dev": "doppler run -- turbo run dev",
		"dev:clean": "pnpm clean && pnpm dev",
		"dev:backend": "doppler run -- pnpm --filter @repo/backend dev",
		"dev:frontend": "doppler run -- pnpm --filter @repo/frontend dev",
		"build": "turbo run build",
		"build:backend": "turbo run build --filter=@repo/backend",
		"build:frontend": "turbo run build --filter=@repo/frontend",
		"build:shared": "turbo run build --filter=@repo/shared",
		"build:database": "turbo run build --filter=@repo/database",
		"build:ci": "turbo run build",
		"lint": "turbo run lint",
		"lint:fix": "turbo run lint:fix",
		"typecheck": "turbo run typecheck",
		"test": "turbo run test:unit test:integration",
		"test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
		"test:unit": "turbo run test:unit",
		"test:unit:backend": "pnpm --filter @repo/backend test:unit",
		"test:unit:frontend": "pnpm --filter @repo/frontend test:unit",
		"test:unit:watch": "pnpm --filter @repo/backend test:unit -- --watch",
		"test:unit:coverage": "pnpm --filter @repo/backend test:unit --coverage",
		"test:integration": "pnpm --filter @repo/backend test:integration",
		"test:integration:watch": "pnpm --filter @repo/backend test:integration -- --watch",
		"test:e2e": "pnpm --filter @repo/e2e-tests test",
		"test:e2e:tenant": "pnpm --filter @repo/e2e-tests test:chromium -- tenant-management/",
		"test:e2e:visual": "pnpm exec playwright test --project=chromium-visual",
		"test:e2e:ui": "pnpm exec playwright test --ui",
		"test:e2e:shard": "pnpm --filter @repo/e2e-tests exec playwright test --shard",
		"test:e2e:debug": "PWDEBUG=1 pnpm exec playwright test",
		"test:production": "./test-production-connection.sh",
		"test:ci": "CI=true turbo run test:unit test:integration --concurrency=2",
		"db:generate": "pnpm --filter @repo/database generate",
		"db:migrate": "doppler run -- pnpm --filter @repo/database migrate",
		"db:studio": "doppler run -- pnpm --filter @repo/database studio",
		"db:push": "doppler run -- supabase db push",
		"db:pull": "doppler run -- supabase db pull",
		"db:reset": "doppler run -- supabase db reset",
		"db:link": "supabase link",
		"db:status": "supabase status",
		"db:migration:new": "supabase migration new",
		"db:migration:list": "supabase migration list",
		"update-supabase-types": "doppler run -- npx supabase gen types typescript --project-id \"bshjmbshupiibfiewpxb\" --schema public > packages/shared/src/types/supabase-generated.ts",
		"clean": "turbo run clean",
		"deps:check": "pnpm audit && pnpm outdated -r",
		"deps:update": "pnpm update -r",
		"check": "manypkg check",
		"fix-deps": "syncpack fix-mismatches",
		"version": "changeset version",
		"release": "changeset publish",
		"workspace:info": "pnpm list -r --depth -1",
		"secrets:generate": "node scripts/manage-secrets.js generate-key",
		"secrets:check": "node scripts/manage-secrets.js check-strength",
		"secrets:export": "node scripts/manage-secrets.js export-for-ci",
		"generate:schemas": "turbo run generate:schemas --filter=@repo/frontend",
		"prepare": "node scripts/prepare-husky.cjs",
		"validate": "pnpm clean && pnpm build:shared && pnpm typecheck && pnpm lint && doppler run -- pnpm --filter @repo/backend test:unit && doppler run -- pnpm test:integration && node scripts/test-backend-health.js && vercel build --no-wait && echo '✅ ALL VALIDATION PASSED - READY TO COMMIT'",
		"validate:quick": "pnpm typecheck && pnpm lint && doppler run -- pnpm test:unit && echo '✅ QUICK VALIDATION PASSED'",
		"validate:clean": "pnpm typecheck && pnpm lint && doppler run -- pnpm --filter @repo/backend test:unit:clean && npx lint-staged --allow-empty --no-stash && echo '✅ CLEAN VALIDATION PASSED'",
		"audit:routes": "tsx scripts/audit-api-routes.ts",
		"stripe:migrate": "doppler run -- pnpm --filter @repo/backend stripe:migrate"
	},
	"optionalDependencies": {
		"@tailwindcss/oxide-linux-arm64-gnu": "4.1.15",
		"@tailwindcss/oxide-linux-arm64-musl": "4.1.15",
		"@tailwindcss/oxide-linux-x64-gnu": "4.1.15",
		"@tailwindcss/oxide-linux-x64-musl": "4.1.15",
		"@tailwindcss/oxide-wasm32-wasi": "4.1.15",
		"lightningcss-linux-x64-gnu": "1.30.2",
		"lightningcss-linux-x64-musl": "1.30.2",
		"turbo-darwin-64": "2.5.8",
		"turbo-darwin-arm64": "2.5.8",
		"turbo-linux-64": "2.5.8",
		"turbo-linux-arm64": "2.5.8"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"msw": {
		"workerDirectory": [
			"../frontend/public"
		]
	},
	"pnpm": {
		"overrides": {
			"typescript": "^5.9.3",
			"path-to-regexp": "^8.3.0",
			"undici": "^7.16.0",
			"tmp": "^0.2.5",
			"glob": "^11.0.3",
			"@types/glob": "^9.0.0",
			"@types/mdx": "^2.0.13",
			"minimatch": "5.1.6",
			"cross-spawn": "^7.0.6",
			"rimraf": "^6.0.1",
			"sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.5",
			"inflight": "npm:lru-cache@^11.2.2",
			"lightningcss": "1.30.2"
		}
	},
	"dependencies": {
		"@changesets/cli": "^2.29.7",
		"@commitlint/cli": "20.1.0",
		"@commitlint/config-conventional": "20.0.0",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "9.38.0",
		"@eslint/object-schema": "2.1.7",
		"@eslint/plugin-kit": "0.4.0",
		"@humanfs/node": "0.16.7",
		"@manypkg/cli": "^0.25.1",
		"@nestjs/terminus": "11.0.0",
		"@playwright/test": "1.56.1",
		"@repo/eslint-config": "workspace:*",
		"@supabase/supabase-js": "catalog:",
		"@types/node": "^24.9.1",
		"babel-plugin-react-compiler": "1.0.0",
		"eslint": "9.38.0",
		"eslint-config-next": "15.5.6",
		"eslint-config-prettier": "10.1.8",
		"eslint-config-turbo": "^2.5.8",
		"eslint-plugin-turbo": "^2.5.8",
		"globals": "16.4.0",
		"husky": "9.1.7",
		"jsdom": "27.0.1",
		"lint-staged": "16.2.6",
		"next": "15.5.6",
		"prettier": "catalog:",
		"prettier-plugin-organize-imports": "4.3.0",
		"syncpack": "^13.0.4",
		"tsx": "^4.20.6",
		"turbo": "2.5.8",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2"
	}
}
