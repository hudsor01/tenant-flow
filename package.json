{
  "name": "tenantflow",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "npm@11.5.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npx turbo run build",
    "build:frontend": "npx turbo run build --filter=@tenantflow/frontend",
    "build:prod": "./scripts/build-production.sh",
    "install:prod": "./scripts/install-production.sh",
    "dev": "npx turbo run dev --parallel",
    "lint": "npx turbo run lint",
    "lint:fix": "npx turbo run lint:fix",
    "check": "npm run lint && npm run typecheck",
    "typecheck": "npx turbo run typecheck",
    "test": "npx turbo run test",
    "test:unit": "npx turbo run test:unit",
    "test:e2e": "npx turbo run test:e2e",
    "generate": "npx turbo gen",
    "format": "npx turbo run format",
    "clean": "npx turbo run clean",
    "claude:check": "(npm run lint && npm run typecheck) || echo 'Fix type/lint errors'",
    "deploy:vercel": "npm run build --filter=@tenantflow/frontend && echo 'Frontend ready for Vercel deployment'",
    "deploy:railway": "TURBO_TELEMETRY_DISABLED=1 npx turbo run build --filter=@tenantflow/backend... --no-daemon",
    "verify:env:frontend": "node scripts/verify-env-vars.js frontend",
    "verify:env:backend": "node scripts/verify-env-vars.js backend",
    "predeploy": "npm run check && npm run test:unit",
    "validate:contracts": "node scripts/validate-api-contracts.js",
    "test:integration": "node scripts/test-frontend-integration.js",
    "monitor:refactoring": "node scripts/monitor-refactoring-impact.js",
    "check:compatibility": "npm run validate:contracts && npm run test:integration",
    "performance:init": "node scripts/performance-orchestrator.js init",
    "performance:monitor": "node scripts/performance-orchestrator.js full",
    "performance:status": "node scripts/performance-orchestrator.js status",
    "performance:dashboard": "node scripts/performance-monitoring-dashboard.js full",
    "performance:build": "node scripts/build-optimization-monitor.js full",
    "performance:cache": "node scripts/performance-monitoring-dashboard.js cache",
    "performance:report": "node scripts/performance-monitoring-dashboard.js report",
    "accelerate:config": "node scripts/prisma-accelerate-config.js all",
    "accelerate:test": "node scripts/prisma-accelerate-config.js test",
    "build:analyze": "node scripts/build-optimization-monitor.js report",
    "build:docker": "node scripts/build-optimization-monitor.js docker",
    "build:turbo": "node scripts/build-optimization-monitor.js turbo",
    "dedup:metrics": "node scripts/simple-deduplication-monitor.js report",
    "dedup:performance": "node scripts/deduplication-performance-monitor.js report",
    "dedup:watch": "node scripts/deduplication-metrics-monitor.js watch",
    "dedup:baseline": "node scripts/simple-deduplication-monitor.js baseline",
    "orchestrator": "node scripts/orchestrator-agent.cjs",
    "orchestrator:validate": "node scripts/orchestrator-agent.cjs validate",
    "orchestrator:dev": "node scripts/orchestrator-agent.cjs dev",
    "orchestrator:build": "node scripts/orchestrator-agent.cjs build",
    "orchestrator:deploy": "node scripts/orchestrator-agent.cjs deploy",
    "orchestrator:test": "node scripts/orchestrator-agent.cjs test",
    "orchestrator:monitor": "node scripts/orchestrator-agent.cjs monitor"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/eslint-plugin-router": "^1.130.12",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@turbo/gen": "^2.5.5",
    "@types/helmet": "^4.0.0",
    "@types/node": "^24.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "allure-playwright": "^3.3.2",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "eslint-config-turbo": "^2.5.5",
    "eslint-plugin-turbo": "^2.5.5",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "plop": "^4.0.1",
    "pluralize": "^8.0.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "path-to-regexp": "^8.2.0",
    "esbuild": "^0.25.8",
    "undici": "^7.13.0",
    "typescript": "5.8.3",
    "minimatch": "^10.0.3"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "vercel": "^44.6.6"
  }
}
