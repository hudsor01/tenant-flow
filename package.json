{
  "name": "tenantflow-turbo",
  "private": true,
  "version": "1.0.0",
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "check": "npm run lint && npm run typecheck",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:e2e": "turbo run test:e2e",
    "test:e2e:headed": "turbo run test:e2e:headed",
    "test:e2e:ui": "turbo run test:e2e:ui",
    "test:all": "turbo run test:all",
    "test:setup": "./scripts/setup-test-env.sh",
    "test:seed": "cd apps/backend && npx tsx ../tests/seed-e2e-data.ts",
    "test:cleanup": "cd apps/backend && npx tsx ../tests/cleanup-e2e-data.ts",
    "generate": "turbo gen",
    "gen:component": "turbo gen component",
    "gen:module": "turbo gen module",
    "gen:type": "turbo gen type",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "clean": "turbo run clean",
    "prisma:studio": "turbo run prisma:studio"
  },
  "dependencies": {
    "@types/dompurify": "^3.0.5",
    "dompurify": "^3.2.6",
    "glob": "^11.0.3",
    "nest-winston": "^1.10.2",
    "resend": "^4.7.0",
    "rxjs": "^7.8.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@playwright/test": "^1.54.1",
    "@supabase/supabase-js": "^2.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@turbo/gen": "^2.5.5",
    "@types/helmet": "^0.0.48",
    "@types/node": "^24.0.14",
    "@vitest/ui": "^3.2.4",
    "allure-playwright": "^3.3.2",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-config-turbo": "^2.5.5",
    "eslint-plugin-turbo": "^2.5.5",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "path-to-regexp": "^8.2.0",
    "esbuild": "^0.25.6",
    "undici": "^7.12.0",
    "zod": "^4.0.5",
    "typescript": "5.8.3"
  }
}
