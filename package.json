{
  "name": "tenant-flow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:full": "concurrently \"npm run db:watch\" \"npm run dev\"",
    "build": "npm run seo:generate && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:types": "node scripts/generate-types-psql.cjs",
    "db:watch": "node scripts/watch-types.cjs",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:reset": "npx supabase db reset",
    "db:migrate": "supabase db push",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration",
    "test:all": "npm run test:unit && npm run test",
    "seo:generate": "npx tsx scripts/generate-robots.ts && npx tsx scripts/generate-sitemap.ts",
    "seo:verify": "test -f public/sitemap.xml && test -f public/robots.txt && echo '✅ SEO files exist' || echo '❌ SEO files missing'",
    "perf:analyze": "node scripts/performance-monitor.js",
    "perf:memory": "node --expose-gc scripts/performance-monitor.js",
    "setup": "chmod +x scripts/setup-dev.sh && ./scripts/setup-dev.sh",
    "setup:stripe": "node scripts/setup-stripe-env.js",
    "setup:hooks": "git config core.hooksPath .githooks && chmod +x .githooks/pre-commit .githooks/pre-push",
    "hooks:install": "npm run setup:hooks",
    "postinstall": "npm run setup:hooks",
    "deploy:functions": "supabase functions deploy create-checkout-session && supabase functions deploy create-subscription && supabase functions deploy create-portal-session && supabase functions deploy cancel-subscription && supabase functions deploy stripe-webhook"
  },
  "overrides": {
    "path-to-regexp": "^6.3.0",
    "esbuild": "^0.25.5",
    "undici": "^5.29.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-email/components": "^0.1.0",
    "@react-email/render": "^1.1.2",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "@supabase/postgrest-js": "^1.19.4",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-query": "^5.81.2",
    "@types/jszip": "^3.4.1",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.18.1",
    "input-otp": "^1.4.2",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.522.0",
    "react": "^19.1.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-email": "^4.0.16",
    "react-hook-form": "^7.58.1",
    "react-resizable-panels": "^3.0.3",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.4",
    "resend": "^4.6.0",
    "sonner": "^2.0.5",
    "stripe": "^18.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "vercel": "^43.3.0",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/ui": "^3.2.4",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
