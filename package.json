{
  "$schema": "https://json.schemastore.org/package",
  "name": "tenantflow",
  "private": true,
  "overrides": {
    "typescript": "5.9.2",
    "esbuild": "^0.25.9",
    "path-to-regexp": "^8.2.0",
    "undici": "^7.15.0",
    "tmp": "^0.2.5",
    "glob": "^11.0.3",
    "@types/glob": "^9.0.0",
    "@types/mdx": "^2.0.13",
    "minimatch": "^10.0.3",
    "cross-spawn": "^7.0.6",
    "rimraf": "^6.0.1",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.5",
    "inflight": "npm:lru-cache@^11.1.0",
    "lightningcss": "1.30.1",
    "@storybook/addon-postcss": {
      "postcss": "^8.5.6"
    }
  },
  "version": "1.0.1",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "arm64",
    "x64"
  ],
  "packageManager": "npm@10.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "turbo": "export $(grep -E '^TURBO_' .env.local 2>/dev/null | xargs) && turbo",
    "start": "cd apps/backend && npm run start",
    "dev": "doppler run -- next dev --turbo",
    "build": "turbo run build",
    "build:frontend": "turbo run build --filter=@repo/shared && turbo run build --filter=@repo/frontend",
    "build:backend": "turbo run build --filter=@repo/backend",
    "build:shared": "turbo run build --filter=@repo/shared",
    "build:validate": "npm run build:shared && node -e \"console.log('Shared package built successfully')\" && npm run build:frontend",
    "build:database": "turbo run build --filter=@repo/database",
    "build:monorepo": "turbo run build --filter='!@repo/frontend'",
    "secrets:generate": "node scripts/manage-secrets.js generate-key",
    "secrets:check": "node scripts/manage-secrets.js check-strength",
    "secrets:export": "node scripts/manage-secrets.js export-for-ci",
    "claude:check": "npm run lint && npm run typecheck",
    "lint": "turbo run lint:fix",
    "format": "turbo run format",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test:unit",
    "test:unit": "turbo run test:unit",
    "test:production": "./test-production-connection.sh",
    "test:api": "cd apps/frontend && npm test -- test/production-api.test.ts",
    "test:visual": "cd apps/storybook && npm run test-visual",
    "test:visual:update": "cd apps/storybook && npm run test-visual:update",
    "test:visual:debug": "cd apps/storybook && npm run test-visual:debug",
    "test:visual:ui": "cd apps/storybook && npm run test-visual:ui",
    "generate": "echo 'Code generation available through individual tools: npm run update-supabase-types, npm run secrets:generate, npm run generate:schemas'",
    "generate:schemas": "turbo run generate:schemas --filter=@repo/frontend",
    "update-supabase-types": "npx supabase gen types typescript --project-id \"bshjmbshupiibfiewpxb\" --schema public > packages/shared/src/types/supabase-generated.ts",
    "db:migration:new": "supabase migration new",
    "db:migration:list": "supabase migration list",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:link": "supabase link",
    "db:status": "supabase status"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.34.0",
    "@eslint/object-schema": "2.1.6",
    "@eslint/plugin-kit": "0.3.5",
    "@humanfs/node": "0.16.6",
    "@playwright/test": "1.55.0",
    "@repo/eslint-config": "*",
    "@tailwindcss/postcss": "4.1.12",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/mocker": "3.2.4",
    "autoprefixer": "10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.2",
    "eslint-config-turbo": "^2.5.6",
    "eslint-plugin-storybook": "0.10.2",
    "eslint-plugin-turbo": "^2.5.6",
    "globals": "16.3.0",
    "jsdom": "^26.1.0",
    "lint-staged": "16.1.5",
    "pre-commit": "1.2.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.0.0-alpha.25",
    "tsx": "4.20.5",
    "turbo": "2.5.6",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@joshwooding/vite-plugin-react-docgen-typescript": "0.6.2",
    "@radix-ui/react-icons": "1.3.2",
    "@rollup/rollup-darwin-arm64": "4.50.0",
    "@storybook/react-docgen-typescript-plugin": "1.0.1",
    "@supabase/ssr": "0.7.0",
    "@supabase/supabase-js": "2.56.1",
    "@swc/helpers": "0.5.17",
    "@tabler/icons-react": "3.34.1",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/typography": "0.5.16",
    "image-size": "2.0.2",
    "react-social-icons": "6.24.0",
    "recharts": "3.1.2",
    "styled-jsx": "5.1.7",
    "vercel": "46.1.0",
    "vite": "7.1.4",
    "vite-tsconfig-paths": "5.1.4"
  },
  "optionalDependencies": {
    "turbo-darwin-64": "2.5.6",
    "turbo-darwin-arm64": "2.5.6",
    "turbo-linux-64": "2.5.6",
    "turbo-linux-arm64": "2.5.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bash -c 'npm run typecheck'"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "msw": {
    "workerDirectory": [
      "../frontend/public"
    ]
  }
}
