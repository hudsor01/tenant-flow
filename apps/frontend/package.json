{
  "$schema": "https://json.schemastore.org/package",
  "name": "@repo/frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#app/*": "./src/app/*",
    "#components/*": "./src/components/*",
    "#contexts/*": "./src/contexts/*",
    "#design-system/*": "./src/design-system/*",
    "#lib/*": "./src/lib/*",
    "#hooks/*": "./src/hooks/*",
    "#stores/*": "./src/stores/*",
    "#types/*": "./src/types/*",
    "#providers/*": "./src/providers/*",
    "#test/*": "./src/test/*",
    "#utils/*": "./src/utils/*"
  },
  "scripts": {
    "dev": "rm -f .next/dev/lock && doppler run -- bash -c 'NEXT_PUBLIC_API_BASE_URL=http://localhost:4650 next dev --turbo --port 3050'",
    "dev:clean": "pnpm clean && pnpm dev",
    "clean": "rm -rf .next",
    "build": "next build",
    "build:local": "doppler run -- next build",
    "analyze": "doppler run -- ANALYZE=true next build --webpack",
    "start": "next start",
    "start:local": "doppler run -- next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ignore-pattern \"**/*.d.ts\" --no-error-on-unmatched-pattern --no-warn-ignored",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix --ignore-pattern \"**/*.d.ts\" --no-error-on-unmatched-pattern --no-warn-ignored",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest --run",
    "test:integration": "doppler run -- bash -c 'RUN_INTEGRATION_TESTS=true vitest --config vitest.integration.config.js --run'",
    "generate:backend-schemas": "node --import tsx scripts/convert-schemas.ts"
  },
