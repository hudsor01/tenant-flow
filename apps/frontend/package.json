{
  "$schema": "https://json.schemastore.org/package",
  "name": "@repo/frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#app/*": "./src/app/*",
    "#components/*": "./src/components/*",
    "#contexts/*": "./src/contexts/*",
    "#design-system/*": "./src/design-system/*",
    "#lib/*": "./src/lib/*",
    "#hooks/*": "./src/hooks/*",
    "#stores/*": "./src/stores/*",
    "#types/*": "./src/types/*",
    "#providers/*": "./src/providers/*",
    "#test/*": "./src/test/*"
  },
  "scripts": {
    "dev": "doppler run -- next dev",
    "dev:clean": "pnpm clean && pnpm dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ignore-pattern \"**/*.d.ts\" --no-error-on-unmatched-pattern --no-warn-ignored",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix --ignore-pattern \"**/*.d.ts\" --no-error-on-unmatched-pattern --no-warn-ignored",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest --run",
    "test:integration": "doppler run -- sh -c 'pnpm --filter @repo/backend dev & BACKEND_PID=$!; if [ $? -ne 0 ]; then echo \"Failed to start backend\"; exit 1; fi; trap \"kill $BACKEND_PID 2>/dev/null || true\" EXIT INT TERM; COUNT=0; MAX_WAIT=60; until curl -s http://localhost:4600/health > /dev/null 2>&1; do COUNT=$((COUNT+1)); if [ $COUNT -gt $MAX_WAIT ]; then echo \"Backend health check timeout after ${MAX_WAIT}s\"; kill $BACKEND_PID 2>/dev/null || true; exit 1; fi; sleep 1; done; vitest --config vitest.integration.config.js --run; TEST_EXIT=$?; kill $BACKEND_PID 2>/dev/null || true; exit $TEST_EXIT'",
    "generate:backend-schemas": "node --import tsx scripts/convert-schemas.ts"
  },
