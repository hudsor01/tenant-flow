{
	"name": "tenantflow-frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"engines": {
		"node": ">=22.x"
	},
	"scripts": {
		"dev": "vite --host --port 5173",
		"dev:clean": "killall node vite 2>/dev/null || true && npm run dev",
		"quick:deploy": "./scripts/quick-deploy.sh",
		"build": "vite build",
		"preview": "vite preview",
		"typecheck": "tsc --noEmit -p tsconfig.build.json",
		"check": "npm run lint && npm run typecheck",
		"lint": "eslint src --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
		"test": "npx vitest run",
		"test:unit": "npx vitest run",
		"test:unit:watch": "npx vitest",
		"test:unit:ui": "npx vitest --ui",
		"test:integration": "npx vitest run tests/integration",
		"test:e2e": "playwright test",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:ui": "playwright test --ui",
		"test:all": "npm run test:unit && npm run test:e2e",
		"seo:generate": "node scripts/generate-seo-sitemap.js",
		"seo:verify": "test -f public/sitemap.xml && test -f public/robots.txt && echo '✅ SEO files exist' || echo '❌ SEO files missing'",
		"seo:verification": "node scripts/generate-verification-files.cjs",
		"perf:analyze": "node scripts/performance-monitor.js",
		"perf:memory": "node --expose-gc scripts/performance-monitor.js",
		"setup": "chmod +x scripts/setup-dev.sh && ./scripts/setup-dev.sh",
		"setup:hooks": "git config core.hooksPath .githooks && chmod +x .githooks/pre-commit .githooks/pre-push",
		"hooks:install": "npm run setup:hooks",
		"deploy:prod": "vite build"
	},
