{
  "name": "@repo/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production next build",
    "export": "next export",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "vitest run --config vitest.a11y.config.ts",
    "test:a11y:watch": "vitest --config vitest.a11y.config.ts",
    "test:a11y:coverage": "vitest run --coverage --config vitest.a11y.config.ts",
    "test:a11y:e2e": "playwright test --config playwright.a11y.config.ts",
    "test:a11y:e2e:headed": "playwright test --config playwright.a11y.config.ts --headed",
    "test:a11y:report": "playwright show-report",
    "preview": "next start",
    "deploy:vercel": "./deploy-vercel.sh",
    "deploy:quick": "./deploy-vercel.sh --skip-tests",
    "deploy:force": "./deploy-vercel.sh --force",
    "predeploy": "npm run type-check && npm run lint",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "performance:analyze": "ANALYZE=true npm run build && echo 'Check .next/analyze/ for bundle analysis'",
    "performance:measure": "npm run build && npm start &",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "a11y:audit": "npm run build && npm run preview & sleep 5 && axe-core-cli http://localhost:3000 --save accessibility-audit.json --dir accessibility-results",
    "a11y:lighthouse": "npm run build && npm run preview & sleep 5 && lhci autorun",
    "a11y:pa11y": "npm run build && npm run preview & sleep 5 && pa11y-ci --sitemap http://localhost:3000/sitemap.xml --standard WCAG2AA"
  },
