{
  "$schema": "https://json.schemastore.org/package",
  "name": "@repo/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "doppler run -- next dev",
    "dev:clean": "rm -rf .next && doppler run -- next dev",
    "dev:webpack": "rm -rf .next && doppler run -- next dev",
    "build": "NAPI_RS_FORCE_WASI=1 NEXT_PUBLIC_SUPABASE_URL=https://placeholder.supabase.co NEXT_PUBLIC_SUPABASE_ANON_KEY=placeholder next build",
    "build:no-schemas": "NAPI_RS_FORCE_WASI=1 next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:tanstack": "./tests/tanstack/run-tanstack-tests.sh all",
    "test:tanstack:optimistic": "./tests/tanstack/run-tanstack-tests.sh optimistic",
    "test:tanstack:infinite": "./tests/tanstack/run-tanstack-tests.sh infinite",
    "test:tanstack:errors": "./tests/tanstack/run-tanstack-tests.sh error",
    "test:tanstack:cache": "./tests/tanstack/run-tanstack-tests.sh cache",
    "test:tanstack:workflows": "./tests/tanstack/run-tanstack-tests.sh workflow",
    "test:tanstack:critical": "./tests/tanstack/run-tanstack-tests.sh critical",
    "test:tanstack:regression": "./tests/tanstack/run-tanstack-tests.sh regression",
    "test:tanstack:performance": "./tests/tanstack/run-tanstack-tests.sh performance",
    "test:tanstack:smoke": "./tests/tanstack/run-tanstack-tests.sh smoke",
    "test:tanstack:debug": "./tests/tanstack/run-tanstack-tests.sh all chrome debug",
    "test:tanstack:mobile": "./tests/tanstack/run-tanstack-tests.sh all mobile",
    "test:dashboard": "./tests/e2e/run-dashboard-tests.sh",
    "test:dashboard:quick": "playwright test dashboard-complete.spec.ts --project=chromium-mvp",
    "test:dashboard:update-snapshots": "playwright test dashboard-complete.spec.ts --update-snapshots",
    "test:media-stack": "./tests/e2e/run-media-stack-tests.sh",
    "test:media-stack:all": "./tests/e2e/run-media-stack-tests.sh all",
    "validate-env": "tsx scripts/validate-env.ts",
    "pre-deploy": "npm run validate-env && npm run lint && npm run typecheck",
    "health-check": "doppler run -- tsx scripts/auth-health-check.ts",
    "generate:schemas": "node --import tsx scripts/generate-frontend-schemas.ts",
    "generate:backend-schemas": "node --import tsx scripts/convert-schemas.ts",
    "claude:check": "npm run lint && npm run typecheck"
  },
