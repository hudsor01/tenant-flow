{
  "name": "@repo/frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.9.3",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:clean": "rm -rf .next node_modules/.cache && next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production next build",
    "export": "next export",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --passWithNoTests",
    "test:unit:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:a11y": "jest --config jest.a11y.config.js",
    "test:a11y:watch": "jest --watch --config jest.a11y.config.js",
    "test:a11y:coverage": "jest --coverage --config jest.a11y.config.js",
    "test:a11y:e2e": "playwright test --config playwright.a11y.config.ts",
    "test:a11y:e2e:headed": "playwright test --config playwright.a11y.config.ts --headed",
    "test:a11y:report": "playwright show-report",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:dashboard": "./tests/run-dashboard-tests.sh",
    "test:dashboard:visual": "./tests/run-dashboard-tests.sh --with-visual",
    "auth:health": "tsx scripts/auth-health-check.ts",
    "auth:health:verbose": "VERBOSE=true tsx scripts/auth-health-check.ts",
    "test:dashboard:mobile": "./tests/run-dashboard-tests.sh --with-mobile",
    "test:dashboard:all": "./tests/run-dashboard-tests.sh --with-visual --with-mobile",
    "preview": "next start",
    "deploy:vercel": "./deploy-vercel.sh",
    "deploy:quick": "./deploy-vercel.sh --skip-tests",
    "deploy:force": "./deploy-vercel.sh --force",
    "predeploy": "npm run type-check && npm run lint",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "performance:analyze": "ANALYZE=true npm run build && echo 'Check .next/analyze/ for bundle analysis'",
    "performance:measure": "npm run build && npm start &",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "a11y:audit": "npm run build && npm run preview & sleep 5 && axe-core-cli http://localhost:3000 --save accessibility-audit.json --dir accessibility-results",
    "a11y:lighthouse": "npm run build && npm run preview & sleep 5 && lhci autorun",
    "a11y:pa11y": "npm run build && npm run preview & sleep 5 && pa11y-ci --sitemap http://localhost:3000/sitemap.xml --standard WCAG2AA"
  },
