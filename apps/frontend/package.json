{
	"$schema": "https://json.schemastore.org/package",
	"name": "@repo/frontend",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "doppler run -- next dev",
		"dev:clean": "pnpm clean && pnpm dev",
		"dev:https": "doppler run -- next dev --experimental-https --experimental-https-key ./certs/localhost+2-key.pem --experimental-https-cert ./certs/localhost+2.pem",
		"build": "next build",
		"start": "next start",
		"start:dev": "doppler run -- next start",
		"start:prod": "NODE_ENV=production pnpm start",
		"lint": "eslint \"src/**/*.{ts,tsx}\" --ignore-pattern \"**/*.d.ts\"",
		"lint:fix": "pnpm lint --fix",
		"typecheck": "tsc --noEmit",
		"test:unit": "jest --testMatch=\"**/*.test.{ts,tsx}\"",
		"test:e2e": "pnpm --filter @repo/e2e-tests test",
		"test:e2e:ui": "pnpm --filter @repo/e2e-tests exec playwright test --ui",
		"clean": "rimraf .next coverage .turbo node_modules/.cache",
		"clean:cache": "rimraf .next/cache && echo 'Webpack cache cleared successfully'",
		"validate-env": "tsx scripts/validate-env.ts",
		"pre-deploy": "pnpm validate-env && pnpm lint && pnpm typecheck",
		"health-check": "doppler run -- tsx scripts/auth-health-check.ts",
		"generate:schemas": "node --import tsx scripts/generate-frontend-schemas.ts",
		"generate:backend-schemas": "node --import tsx scripts/convert-schemas.ts"
	},
