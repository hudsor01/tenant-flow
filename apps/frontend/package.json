{
  "$schema": "https://json.schemastore.org/package",
  "name": "@repo/frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.0.0",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "dev:clean": "rm -rf .next && next dev --turbo",
    "dev:webpack": "rm -rf .next && next dev",
    "build": "next build",
    "build:no-schemas": "next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "npx jest --passWithNoTests",
    "test:unit": "npx jest --passWithNoTests",
    "test:e2e": "playwright test",
    "test:dashboard": "./tests/e2e/run-dashboard-tests.sh",
    "test:dashboard:quick": "playwright test dashboard-complete.spec.ts --project=chromium-mvp",
    "test:dashboard:update-snapshots": "playwright test dashboard-complete.spec.ts --update-snapshots",
    "validate-env": "tsx scripts/validate-env.ts",
    "pre-deploy": "npm run validate-env && npm run lint && npm run typecheck",
    "health-check": "tsx scripts/auth-health-check.ts",
    "generate:schemas": "node --import tsx scripts/generate-frontend-schemas.ts",
    "claude:check": "npm run lint && npm run typecheck"
  },
