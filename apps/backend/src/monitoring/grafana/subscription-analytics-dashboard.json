{
  "dashboard": {
    "id": null,
    "title": "TenantFlow - Subscription Analytics",
    "tags": ["tenantflow", "subscriptions", "revenue"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Monthly Recurring Revenue (MRR)",
        "type": "stat",
        "targets": [
          {
            "expr": "subscription_mrr_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Subscriptions",
        "type": "stat",
        "targets": [
          {
            "expr": "subscription_count_total{status=\"active\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Trial Conversion Rate (30d)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(subscription_trial_conversions_total[30d]) / increase(subscription_count_total{status=\"trialing\"}[30d]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 15},
                {"color": "green", "value": 25}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Churn Rate (30d)",
        "type": "stat",
        "targets": [
          {
            "expr": "business_churn_rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "MRR Growth Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "subscription_mrr_total",
            "legendFormat": "Current MRR",
            "refId": "A"
          },
          {
            "expr": "deriv(subscription_mrr_total[30d]) * 86400 * 30",
            "legendFormat": "Monthly Growth",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Subscription Status Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status) (subscription_count_total)",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "New Subscriptions vs Cancellations",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(subscription_created_total[1h])",
            "legendFormat": "New Subscriptions",
            "refId": "A"
          },
          {
            "expr": "rate(subscription_cancelled_total[1h])",
            "legendFormat": "Cancellations",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Subscription Plans Distribution",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (plan_type) (subscription_count_total{status=\"active\"})",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Customer Lifetime Value",
        "type": "stat",
        "targets": [
          {
            "expr": "business_customer_lifetime_value",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Trial to Paid Conversion Funnel",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(subscription_count_total{status=\"trialing\"}[1h])",
            "legendFormat": "Trial Starts",
            "refId": "A"
          },
          {
            "expr": "rate(subscription_trial_conversions_total[1h])",
            "legendFormat": "Trial Conversions",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "refresh": "5m"
  }
}