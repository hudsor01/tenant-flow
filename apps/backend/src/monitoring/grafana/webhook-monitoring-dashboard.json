{
  "dashboard": {
    "id": null,
    "title": "TenantFlow - Webhook Monitoring",
    "tags": ["tenantflow", "webhooks", "stripe"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Webhook Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(webhook_events_total{status=\"success\"}[5m]) / rate(webhook_events_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Webhook Volume (Last 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(webhook_events_total[24h])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Failed Webhooks (Last 1h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(webhook_events_total{status=\"failed\"}[1h])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Webhook Processing Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, webhook_processing_duration_seconds_bucket)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Webhook Events by Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(webhook_events_total[5m]) by (event_type)",
            "legendFormat": "{{event_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Webhook Status Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status) (increase(webhook_events_total[24h]))",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Failed Webhook Events by Type",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (event_type, error_type) (increase(webhook_events_total{status=\"failed\"}[24h]))",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Webhook Response Time Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(webhook_processing_duration_seconds_bucket[5m])",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s"
  }
}