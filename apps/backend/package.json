{
	"$schema": "https://json.schemastore.org/package",
	"name": "@repo/backend",
	"version": "0.0.2",
	"private": true,
	"license": "UNLICENSED",
	"engines": {
		"node": ">=22.14.0",
		"pnpm": ">=9"
	},
	"scripts": {
		"build": "doppler run -- tsc -p tsconfig.build.json",
		"dev": "doppler run -- nest start --watch",
		"start": "node dist/main.js",
		"start:prod": "node dist/main.js",
		"lint": "doppler run -- eslint . --fix",
		"typecheck": "tsc -p tsconfig.build.json --noEmit --incremental --skipLibCheck",
		"test": "npx jest",
		"test:unit": "npx jest --testPathIgnorePatterns=\"integration|e2e\" --passWithNoTests",
		"test:integration": "npx jest --testMatch=\"**/*.integration.spec.ts\" --passWithNoTests",
		"test:doppler": "doppler run -- npx jest",
		"clean": "rm -rf dist .turbo"
	},
	"dependencies": {
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"express-rate-limit": "^7.4.1",
		"helmet": "^8.0.0",
		"express": "^5.0.1",
		"multer": "^1.4.5-lts.1",
		"@nestjs/common": "^11.1.6",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.1.6",
		"@nestjs/event-emitter": "^3.0.1",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/platform-express": "^11.1.6",
		"@nestjs/swagger": "^11.2.0",
		"@nestjs/terminus": "11.0.0",
		"@nestjs/throttler": "6.4.0",
		"@react-email/components": "0.5.3",
		"@repo/shared": "workspace:*",
		"react": "^19.1.1",
		"@supabase/stripe-sync-engine": "0.44.0",
		"@supabase/supabase-js": "^2.57.4",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.2",
		"handlebars": "4.7.8",
		"nest-commander": "3.20.0",
		"nestjs-pino": "4.4.0",
		"pino-http": "10.5.0",
		"posthog-js": "catalog:",
		"puppeteer": "^24.22.0",
		"reflect-metadata": "0.2.2",
		"resend": "6.1.0",
		"rxjs": "^7.8.2",
		"stripe": "^18.5.0",
		"uuid": "13.0.0",
		"zod": "4.1.11"
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^5.0.0",
		"@types/multer": "^1.4.12",
		"@golevelup/ts-jest": "0.7.0",
		"@nestjs/cli": "^11.0.10",
		"@nestjs/schematics": "^11.0.7",
		"@nestjs/testing": "^11.1.6",
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@types/jest": "30.0.0",
		"@types/node": "^24.5.2",
		"@types/react": "catalog:",
		"@types/supertest": "^6.0.3",
		"eslint": "9.36.0",
		"globals": "16.4.0",
		"jest": "^30.1.3",
		"pino-pretty": "13.1.1",
		"supertest": "^7.1.4",
