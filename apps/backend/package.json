{
	"$schema": "https://json.schemastore.org/package",
	"name": "@repo/backend",
	"version": "0.0.2",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"dev": "NODE_ENV=development NODE_NO_WARNINGS=1 nest start --watch",
		"dev:clean": "pnpm clean && pnpm dev",
		"build": "nest build",
		"start": "node dist/main.js",
		"start:dev": "node dist/main.js",
		"start:prod": "NODE_ENV=production node dist/main.js",
		"lint": "eslint \"src/**/*.ts\" --cache --cache-location node_modules/.cache/eslint/ --ignore-pattern \"**/*.d.ts\" --no-error-on-unmatched-pattern --no-warn-ignored",
		"lint:fix": "pnpm lint --fix",
		"typecheck": "tsc --noEmit",
		"test": "pnpm run test:unit",
		"test:unit": "jest --testMatch=\"**/*.spec.ts\" --testPathIgnorePatterns=\"\\.integration\\.spec\\.ts$\" --forceExit",
		"test:integration": "jest --testMatch=\"**/*.integration.spec.ts\" --testPathIgnorePatterns=\"/node_modules/|/dist/|/coverage/\" --forceExit --passWithNoTests",
		"test:integration:strict": "RUN_INTEGRATION_TESTS=true jest --testMatch=\"**/*.integration.spec.ts\" --testPathIgnorePatterns=\"/node_modules/|/dist/|/coverage/\" --forceExit",
		"test:all": "pnpm run test:unit && pnpm run test:integration",
		"test:watch": "jest --watch",
		"clean": "rimraf dist coverage .turbo node_modules/.cache",
		"stripe:migrate": "tsx scripts/stripe-sync-migrate.ts",
		"stripe:backfill-customers": "tsx scripts/backfill-stripe-customers.ts",
		"validate:supabase": "tsx scripts/validate-supabase-keys.ts"
	},
