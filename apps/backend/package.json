		"supertest": "^7.1.4",
		"ts-jest": "29.4.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.5",
		"typescript": "5.9.2",
    "typescript-eslint": "8.40.0"
  },
	"overrides": {
		"path-to-regexp": "^8.2.0",
		"minimatch": "^3.1.2"
	},
	"optionalDependencies": {
		"@esbuild/darwin-arm64": "0.25.9"
	}
}
