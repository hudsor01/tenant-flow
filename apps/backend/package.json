{
	"$schema": "https://json.schemastore.org/package",
	"name": "@repo/backend",
	"version": "0.0.2",
	"private": true,
	"license": "UNLICENSED",
	"packageManager": "npm@10.0.0",
	"engines": {
		"node": ">=20.0.0",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"dev": "tsx watch src/main.ts",
		"start": "node dist/main.js",
		"start:prod": "node dist/main.js",
		"lint": "eslint . --fix --no-warn-ignored",
		"typecheck": "tsc -p tsconfig.build.json --noEmit --incremental --skipLibCheck",
		"test": "jest",
		"test:unit": "jest --testPathIgnorePatterns=\"integration|e2e\" --passWithNoTests",
		"test:integration": "jest --testMatch=\"**/*.integration.spec.ts\" --passWithNoTests",
		"clean": "rm -rf dist .turbo",
		"claude:check": "npm run lint && npm run typecheck"
	},
	"dependencies": {
		"@fastify/circuit-breaker": "^4.0.2",
		"@fastify/compress": "^8.1.0",
		"@fastify/cookie": "^11.0.2",
		"@fastify/csrf-protection": "^7.1.0",
		"@fastify/env": "^5.0.2",
		"@fastify/etag": "^6.0.3",
		"@fastify/helmet": "^13.0.1",
		"@fastify/multipart": "^9.0.3",
		"@fastify/rate-limit": "^10.3.0",
		"@fastify/request-context": "^6.2.1",
		"@fastify/sensible": "6.0.3",
		"@fastify/under-pressure": "9.0.3",
		"@nestjs/cache-manager": "3.0.1",
		"@nestjs/common": "^11.1.6",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.1.6",
		"@nestjs/event-emitter": "^3.0.1",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/platform-fastify": "11.1.6",
		"@nestjs/schedule": "^6.0.0",
		"@nestjs/swagger": "^11.2.0",
		"@nestjs/terminus": "11.0.0",
		"@nestjs/throttler": "6.4.0",
		"@react-email/components": "0.5.1",
		"@repo/database": "*",
		"@repo/shared": "*",
		"@supabase/supabase-js": "^2.56.0",
		"bcrypt": "^6.0.0",
		"check-disk-space": "3.4.0",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.2",
		"dotenv": "^17.2.1",
		"dotenv-flow": "^4.1.0",
		"handlebars": "4.7.8",
		"jsonwebtoken": "^9.0.2",
		"nest-commander": "3.19.0",
		"nestjs-pino": "4.4.0",
		"pino-http": "10.5.0",
		"prom-client": "15.1.3",
		"puppeteer": "^24.17.1",
		"reflect-metadata": "0.2.2",
		"resend": "6.0.1",
		"rxjs": "^7.8.2",
		"stripe": "^18.5.0",
		"zod": "4.1.0"
	},
	"devDependencies": {
		"@golevelup/ts-jest": "0.7.0",
		"@nestjs/cli": "^11.0.10",
		"@nestjs/schematics": "^11.0.7",
		"@nestjs/testing": "^11.1.6",
		"@repo/eslint-config": "*",
		"@repo/typescript-config": "*",
		"@types/bcrypt": "^6.0.0",
		"@types/jest": "30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
