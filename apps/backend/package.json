	"exports": {
		".": "./dist/main.js"
	},
	"scripts": {
		"build": "npm run generate && nest build",
		"dev": "prisma generate && tsx watch src/main.ts",
		"dev:debug": "prisma generate && NODE_OPTIONS='--enable-source-maps' tsx watch --inspect src/main.ts",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint . --ext .ts --fix --ignore-pattern 'src/test/setup.ts'",
		"lint:check": "eslint . --ext .ts --ignore-pattern 'src/test/setup.ts'",
		"format": "prettier --write \"src/**/*.ts\"",
		"format:check": "prettier --check \"src/**/*.ts\"",
		"typecheck": "NODE_OPTIONS='--max-old-space-size=8192' tsc --noEmit --skipLibCheck -p tsconfig.json",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"generate": "prisma generate",
		"prisma:studio": "prisma studio"
	},
