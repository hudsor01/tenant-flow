		"exceljs": "4.4.0",
		"express": "^5.2.1",
		"handlebars": "4.7.8",
		"helmet": "^8.1.0",
		"i18n-iso-countries": "7.14.0",
		"jose": "6.1.3",
		"jsonwebtoken": "^9.0.3",
		"jwt-decode": "4.0.0",
		"multer": "2.0.2",
		"nest-winston": "1.10.2",
		"nestjs-cls": "6.1.0",
