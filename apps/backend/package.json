{
	"$schema": "https://json.schemastore.org/package",
	"name": "@repo/backend",
	"version": "0.0.2",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"dev": "NODE_NO_WARNINGS=1 doppler run -- nest start --watch",
		"dev:clean": "pnpm clean && pnpm dev",
		"build": "nest build",
		"start": "doppler run -- node dist/main.js",
		"start:dev": "doppler run -- node dist/main.js",
		"start:prod": "doppler run -- NODE_ENV=production node dist/main.js",
		"lint": "eslint \"src/**/*.ts\" --ignore-pattern \"**/*.d.ts\" --no-error-on-unmatched-pattern --no-warn-ignored",
		"lint:fix": "pnpm lint --fix",
		"typecheck": "tsc --noEmit",
		"test": "pnpm run test:unit",
		"test:unit": "jest --testMatch=\"**/*.spec.ts\" --testPathIgnorePatterns=\"\\.integration\\.spec\\.ts$\" --forceExit",
		"test:integration": "doppler run -- jest --testMatch=\"**/*.integration.spec.ts\" --testPathIgnorePatterns=\"/node_modules/|/dist/|/coverage/\" --forceExit --passWithNoTests",
		"test:integration:strict": "doppler run -- RUN_INTEGRATION_TESTS=true jest --testMatch=\"**/*.integration.spec.ts\" --testPathIgnorePatterns=\"/node_modules/|/dist/|/coverage/\" --forceExit",
		"test:all": "pnpm run test:unit && doppler run -- pnpm run test:integration",
		"test:watch": "jest --watch",
		"clean": "rimraf dist coverage .turbo node_modules/.cache",
		"stripe:migrate": "doppler run -- tsx scripts/stripe-sync-migrate.ts",
		"stripe:backfill-customers": "doppler run -- tsx scripts/backfill-stripe-customers.ts",
		"validate:supabase": "doppler run -- tsx scripts/validate-supabase-keys.ts"
	},
	"dependencies": {
		"@bull-board/api": "6.15.0",
		"@bull-board/express": "6.15.0",
		"@bull-board/nestjs": "6.15.0",
		"@nestjs/bullmq": "11.0.4",
		"@nestjs/cache-manager": "3.0.1",
		"@nestjs/common": "^11.1.9",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.1.9",
		"@nestjs/event-emitter": "^3.0.1",
		"@nestjs/jwt": "^11.0.1",
		"@nestjs/platform-express": "^11.1.9",
		"@nestjs/schedule": "6.0.1",
		"@nestjs/terminus": "11.0.0",
		"@nestjs/throttler": "6.4.0",
		"@react-email/components": "1.0.1",
		"@react-email/render": "2.0.0",
		"@react-pdf/renderer": "4.3.1",
		"@repo/shared": "workspace:*",
		"@supabase/stripe-sync-engine": "0.45.0",
		"@supabase/supabase-js": "catalog:default",
		"@willsoto/nestjs-prometheus": "6.0.2",
		"bcrypt": "6.0.0",
		"bullmq": "5.63.2",
		"cache-manager": "7.2.5",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.2",
