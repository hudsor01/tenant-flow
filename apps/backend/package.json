		"compression": "^1.8.1",
		"cookie-parser": "^1.4.7",
		"express": "^5.1.0",
		"express-rate-limit": "^8.1.0",
		"handlebars": "4.7.8",
		"helmet": "^8.1.0",
		"multer": "^2.0.2",
		"nest-commander": "3.20.0",
		"nestjs-cls": "6.0.1",
		"posthog-js": "catalog:",
		"puppeteer": "^24.22.2",
		"react": "^19.1.1",
		"reflect-metadata": "0.2.2",
