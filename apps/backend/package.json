{
	"name": "@tenantflow/backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "NODE_ENV=production npm run generate && NODE_OPTIONS='--max-old-space-size=4096' npx nest build",
		"build:railway": "NODE_ENV=production npm run generate && NODE_OPTIONS='--max-old-space-size=4096' npm run build:tsc",
		"build:docker": "NODE_ENV=production npm run generate && NODE_OPTIONS='--max-old-space-size=4096' npx nest build",
		"build:tsc": "NODE_OPTIONS='--max-old-space-size=4096' tsc -p tsconfig.build.json",
		"dev": "NODE_OPTIONS='--max-old-space-size=4096' prisma generate && tsx watch src/main.ts",
		"dev:debug": "prisma generate && NODE_OPTIONS='--enable-source-maps --max-old-space-size=4096' tsx watch --inspect src/main.ts",
		"start": "npx nest start",
		"start:prod": "npm run migrate:deploy && node dist/main.js",
		"start:railway": "prisma migrate deploy && node --max-old-space-size=2048 dist/main.js",
		"lint": "eslint . --ext .ts --fix --ignore-pattern 'src/test/setup.ts'",
		"format": "prettier --write \"src/**/*.ts\"",
		"typecheck": "npm run generate && NODE_OPTIONS='--max-old-space-size=8192' tsc -p tsconfig.build.json --noEmit --incremental --skipLibCheck",
		"test": "vitest run",
		"test:unit": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"generate": "NODE_OPTIONS='--max-old-space-size=2048' prisma generate",
		"migrate:deploy": "prisma migrate deploy",
		"migrate:dev": "prisma migrate dev",
		"prisma:studio": "prisma studio"
	},
	"dependencies": {
		"@fastify/circuit-breaker": "^4.0.2",
