{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"buildCommand": "npm run db:generate && npm run build:shared && cd apps/frontend && npm run build",
	"installCommand": "npm ci --omit=dev --omit=optional",
	"framework": "nextjs",
	"outputDirectory": "apps/frontend/.next",
	"env": {
		"npm_config_target_platform": "linuxmusl",
		"npm_config_target_arch": "x64"
	},
	"functions": {
		"apps/frontend/**/*.js": {
			"maxDuration": 10
		}
	},
	"crons": []
}
