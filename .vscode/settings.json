{
  "editor.scrollbar.vertical": "visible",
  "editor.scrollbar.horizontal": "visible",
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.scrollbar.horizontalScrollbarSize": 8,
  "editor.scrollbar.scrollByPage": false,
  "workbench.colorCustomizations": {
    "scrollbarSlider.background": "#666666FF",
    "scrollbarSlider.hoverBackground": "#999999FF",
    "scrollbarSlider.activeBackground": "#CCCCCCFF",
    "scrollbar.shadow": "#000000AA"
  },
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": false,
  "editor.fastScrollSensitivity": 5,
  "editor.mouseWheelScrollSensitivity": 1,
  "files.participants.timeout": 1000,
  "files.exclude": {},
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.enableSmartCommit": false,
  "git.postCommitCommand": "none",
  "prettier.enable": false,
  "eslint.enable": false,
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.suggest.autoImports": false,
  "eslint.workingDirectories": [
    { "mode": "auto" }
  ],
  "editor.formatOnSave": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "files.associations": {
    "turbo.json": "jsonc"
  },
  "json.schemas": [],
  "json.schemaDownload.enable": false,
  "accessibility.signals.save": {
    "sound": "off"
  },
  "accessibility.signals.format": {
    "sound": "off"
  },
  "editor.formatOnSaveTimeout": 100,
  "editor.codeActionsOnSaveTimeout": 100,
  "editor.codeActionsOnSave": {},
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/.turbo": true,
    "**/coverage": true,
    "**/.next": true,
    "**/build": true
  },
  "explorer.fileNesting.enabled": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.test.ts, ${capture}.spec.ts, ${capture}.d.ts",
    "*.tsx": "${capture}.test.tsx, ${capture}.spec.tsx, ${capture}.stories.tsx",
    "*.js": "${capture}.test.js, ${capture}.spec.js",
    "*.jsx": "${capture}.test.jsx, ${capture}.spec.jsx",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, .npmrc",
    "turbo.json": ".turbo"
  },
"todo-tree.tree.scanMode": "workspace only",
"chat.mcp.serverSampling": {
  "tenant-flow/.vscode/mcp.json: MCP_DOCKER": {
    "allowedModels": [
      "github.copilot-chat/gpt-4.1",
      "github.copilot-chat/claude-3.5-sonnet",
      "github.copilot-chat/claude-3.7-sonnet",
      "github.copilot-chat/claude-3.7-sonnet-thought",
      "github.copilot-chat/claude-sonnet-4",
      "github.copilot-chat/gemini-2.0-flash-001",
      "github.copilot-chat/gemini-2.5-pro",
      "github.copilot-chat/gpt-4o",
      "github.copilot-chat/o3-mini",
      "github.copilot-chat/o4-mini"
    ]
  }
},
"explorer.fileNesting.expand": false,
"containers.containers.groupBy": "Networks",
"containers.volumes.groupBy": "VolumeName",
"containers.orchestratorClient": "com.microsoft.visualstudio.orchestrators.dockercompose",
"containers.networks.groupBy": "NetworkName",
"containers.contexts.showInStatusBar": true,
"yaml.keyOrdering": false,
"yaml.format.singleQuote": true,
"yaml.format.bracketSpacing": false,
"[typescriptreact]": {
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
},
"[typescript]": {
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
},
"[json]": {
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
},
"[javascript]": {
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
},
"github.copilot.enable": {
  "javascript": true
},
"github.copilot.selectedCompletionModel": "",
"github.copilot.chat.terminalChatLocation": "terminal",
"github.copilot.nextEditSuggestions.enabled": true,
"github.copilot.chat.byok.ollamaEndpoint": "http://localhost:11434/api/generate",
"github.copilot.chat.codesearch.enabled": true,
"github.copilot.chat.agent.thinkingTool": true,
"github.copilot.chat.completionContext.typescript.mode": "on",
"github.copilot.chat.editor.temporalContext.enabled": true,
"github.copilot.chat.edits.temporalContext.enabled": true,
"github.copilot.chat.generateTests.codeLens": true,
"github.copilot.chat.languageContext.fix.typescript.enabled": true,
"github.copilot.chat.languageContext.inline.typescript.enabled": true,
"github.copilot.chat.languageContext.typescript.enabled": true,
"github.copilot.chat.notebook.followCellExecution.enabled": true,
"notebook.defaultFormatter": "rvest.vs-code-prettier-eslint",
"githubPullRequests.experimental.useQuickChat": true,
"github.copilot.chat.localeOverride": "en",
"workbench.editor.autoLockGroups": {
  "workbench.editor.chatSession": false
},
"workbench.editor.alwaysShowEditorActions": false,
"workbench.editor.centeredLayoutFixedWidth": true,
"workbench.editor.closeOnFileDelete": true,
"workbench.editor.defaultBinaryEditor": "default",
"workbench.editor.enablePreviewFromQuickOpen": true,
"workbench.editor.enablePreviewFromCodeNavigation": true,
"workbench.editor.highlightModifiedTabs": false,
"versionlens.caching.duration": 5,
"versionlens.cargo.caching.duration": 5,
"versionlens.composer.caching.duration": 5,
"versionlens.deno.caching.duration": 5,
"versionlens.docker.caching.duration": 5,
"versionlens.dotnet.caching.duration": 5,
"versionlens.dotnet.onSaveChanges": "",
"versionlens.dub.caching.duration": 5,
"versionlens.golang.caching.duration": 5,
"versionlens.maven.caching.duration": 5,
"versionlens.npm.caching.duration": 5,
"versionlens.pub.caching.duration": 5,
"versionlens.pypi.caching.duration": 5,
"versionlens.suggestions.showOnStartup": true,
"versionlens.suggestions.showSuggestionsStats": true,
"[jsonc]": {
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
},
"github.copilot.chat.enableRetryAfterFilteredResponse": true,
"github.copilot.chat.newWorkspace.useContext7": true,
"[markdown]": {
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
},
"editor.formatOnSaveMode": "modifications"
}
