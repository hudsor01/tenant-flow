{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "TenantFlow Next.js TypeScript Config - App base",
  "extends": "./base.json",
  "compilerOptions": {
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "noEmit": true,
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": false
  }
}
