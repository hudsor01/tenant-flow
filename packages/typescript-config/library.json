{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "TenantFlow Library TypeScript Config",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2022",
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "isolatedModules": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}