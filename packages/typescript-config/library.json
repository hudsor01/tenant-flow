{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "TenantFlow Frontend Library TypeScript Config",
	"extends": "./base.json",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"composite": true,
		"jsx": "react-jsx",
		"removeComments": false,
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"verbatimModuleSyntax": false
	}
}
