{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "TenantFlow Node.js Library TypeScript Config",
	"extends": "./base.json",
	"compilerOptions": {
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ES2022",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"composite": true,
		"removeComments": false,
		"isolatedModules": false,
		"verbatimModuleSyntax": false
	}
}
