{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "TenantFlow Node.js Library TypeScript Config",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "noEmit": false,
    "composite": false,
    "removeComments": false,
    "isolatedModules": false,
    "target": "ES2022"
  }
}
