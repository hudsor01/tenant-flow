{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "TenantFlow API Client Package Config",
	"extends": "../typescript-config/library.json",
	"compilerOptions": {
		"rootDir": "./src",
		"outDir": "./dist",
		"composite": true,
		"tsBuildInfoFile": "./dist/.tsbuildinfo"
	},
	"include": ["src/**/*.ts"],
	"exclude": ["dist", "node_modules", "**/*.test.*", "**/*.spec.*"],
	"references": [
		{ "path": "../config" },
		{ "path": "../shared" }
	]
}
