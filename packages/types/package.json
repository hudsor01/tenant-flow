{
  "name": "@tenantflow/types",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.build.json",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@tenantflow/typescript-config": "*"
  }
}
