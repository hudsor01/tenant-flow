{
	"$schema": "https://json.schemastore.org/package",
	"name": "@repo/database",
	"version": "0.1.0",
	"private": true,
	"packageManager": "npm@10.0.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "npm run clean-dist && tsc --build",
		"clean": "tsc --build --clean && rm -rf dist",
		"clean-dist": "tsc --build --clean && rm -rf dist",
		"typecheck": "tsc --noEmit",
		"lint": "eslint . --fix",
		"lint:fix": "eslint . --fix",
		"format": "echo 'No formatting configured for database package'",
		"format:check": "echo 'No formatting configured for database package'"
	},
	"devDependencies": {
		"@repo/typescript-config": "*",
		"eslint": "9.34.0",
		"globals": "16.3.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "8.40.0"
	},
	"overrides": {
		"minimatch": "^3.1.2"
	},
	"dependencies": {
		"@repo/shared": "*",
		"@supabase/supabase-js": "^2.56.0"
	}
}
