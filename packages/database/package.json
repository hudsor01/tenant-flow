{
  "name": "@repo/database",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean-dist && tsc --build",
    "clean": "tsc --build --clean && rm -rf dist",
    "clean-dist": "tsc --build --clean && rm -rf dist",
    "format": "echo 'No formatting configured for database package'",
    "format:check": "echo 'No formatting configured for database package'",
    "lint": "eslint . --fix",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "$schema": "https://json.schemastore.org/package",
  "dependencies": {
    "@repo/shared": "*",
    "@supabase/supabase-js": "^2.57.4",
    "@types/node": "^24.3.1"
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "eslint": "^9.35.0",
    "globals": "16.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "overrides": {
    "minimatch": "^10.0.3"
  },
  "packageManager": "npm@11.6.0",
  "private": true,
  "type": "module"
}
