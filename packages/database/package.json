{
	"name": "@repo/database",
	"version": "0.1.0",
	"private": true,
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "npm run clean-dist && tsc --build",
		"clean": "tsc --build --clean && rm -rf dist",
		"clean-dist": "tsc --build --clean && rm -rf dist",
		"typecheck": "tsc --noEmit",
		"lint": "eslint . --max-warnings 0 --no-warn-ignored",
		"lint:fix": "eslint . --fix",
		"format": "echo 'No formatting configured for database package'",
		"format:check": "echo 'No formatting configured for database package'"
	},
	"devDependencies": {
		"@repo/typescript-config": "*",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"@repo/shared": "*",
		"@supabase/supabase-js": "^2.55.0"
	}
}
