{
	"name": "@repo/shared",
	"version": "0.1.0",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"typesVersions": {
		"*": {
			"types/*": [
				"./dist/cjs/types/*"
			]
		}
	},
	"private": true,
	"exports": {
		".": {
			"types": "./dist/cjs/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"default": "./dist/esm/index.js"
		},
		"./types": {
			"types": "./dist/cjs/types/index.d.ts",
			"import": "./dist/esm/types/index.js",
			"require": "./dist/cjs/types/index.js"
		},
		"./types/*": {
			"types": "./dist/cjs/types/*.d.ts",
			"import": "./dist/esm/types/*.js",
			"require": "./dist/cjs/types/*.js"
		},
		"./constants": {
			"types": "./dist/cjs/constants/index.d.ts",
			"import": "./dist/esm/constants/index.js",
			"require": "./dist/cjs/constants/index.js"
		},
		"./constants/*": {
			"types": "./dist/cjs/constants/*.d.ts",
			"import": "./dist/esm/constants/*.js",
			"require": "./dist/cjs/constants/*.js"
		},
		"./utils": {
			"types": "./dist/cjs/utils/index.d.ts",
			"import": "./dist/esm/utils/index.js",
			"require": "./dist/cjs/utils/index.js"
		},
		"./utils/*": {
			"types": "./dist/cjs/utils/*.d.ts",
			"import": "./dist/esm/utils/*.js",
			"require": "./dist/cjs/utils/*.js"
		},
		"./validation": {
			"types": "./dist/cjs/validation/index.d.ts",
			"import": "./dist/esm/validation/index.js",
			"require": "./dist/cjs/validation/index.js"
		},
		"./validation/*": {
			"types": "./dist/cjs/validation/*.d.ts",
			"import": "./dist/esm/validation/*.js",
			"require": "./dist/cjs/validation/*.js"
		},
		"./domain": {
			"types": "./dist/cjs/types/domain.d.ts",
			"import": "./dist/esm/types/domain.js",
			"require": "./dist/cjs/types/domain.js"
		},
		"./utilities": {
			"types": "./dist/cjs/types/utilities.d.ts",
			"import": "./dist/esm/types/utilities.js",
			"require": "./dist/cjs/types/utilities.js"
		},
		"./config": {
			"types": "./dist/cjs/config/index.d.ts",
			"import": "./dist/esm/config/index.js",
			"require": "./dist/cjs/config/index.js"
		},
		"./config/*": {
			"types": "./dist/cjs/config/*.d.ts",
			"import": "./dist/esm/config/*.js",
			"require": "./dist/cjs/config/*.js"
		}
	},
	"scripts": {
		"build": "npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc -p tsconfig.build.json",
		"build:esm": "tsc -p tsconfig.esm.json",
		"typecheck": "tsc --noEmit",
		"check-types": "tsc --noEmit",
		"lint": "eslint . --ext .ts --max-warnings 0",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"format:check": "prettier --check \"src/**/*.ts\"",
		"clean": "tsc --build --clean && rm -rf dist"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.55.0",
		"@tanstack/react-query": "^5.85.3",
		"react-hook-form": "^7.62.0",
		"zod": "^4.0.17"
	},
	"devDependencies": {
		"@repo/typescript-config": "*",
		"@types/node": "^24.3.0",
		"@types/react": "^19.1.10",
		"stripe": "^18.4.0",
		"typescript": "^5.9.2"
	},
	"optionalDependencies": {
		"@esbuild/linux-x64": "0.25.9",
		"@rollup/rollup-linux-x64-gnu": "4.46.2"
	}
}
