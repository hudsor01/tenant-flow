{
  "name": "@tenantflow/shared",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    },
    "./types/auth": {
      "types": "./dist/types/auth.d.ts",
      "import": "./dist/types/auth.js",
      "require": "./dist/types/auth.js"
    },
    "./types/billing": {
      "types": "./dist/types/billing.d.ts",
      "import": "./dist/types/billing.js",
      "require": "./dist/types/billing.js"
    },
    "./types/properties": {
      "types": "./dist/types/properties.d.ts",
      "import": "./dist/types/properties.js",
      "require": "./dist/types/properties.js"
    },
    "./types/maintenance": {
      "types": "./dist/types/maintenance.d.ts",
      "import": "./dist/types/maintenance.js",
      "require": "./dist/types/maintenance.js"
    },
    "./types/leases": {
      "types": "./dist/types/leases.d.ts",
      "import": "./dist/types/leases.js",
      "require": "./dist/types/leases.js"
    },
    "./types/tenants": {
      "types": "./dist/types/tenants.d.ts",
      "import": "./dist/types/tenants.js",
      "require": "./dist/types/tenants.js"
    },
    "./types/errors": {
      "types": "./dist/types/errors.d.ts",
      "import": "./dist/types/errors.js",
      "require": "./dist/types/errors.js"
    },
    "./validation": {
      "types": "./dist/validation/index.d.ts",
      "import": "./dist/validation/index.js",
      "require": "./dist/validation/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.build.json",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@tenantflow/typescript-config": "*"
  }
}