{
  "name": "@repo/shared",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.js"
    },
    "./constants/*": {
      "types": "./dist/constants/*.d.ts",
      "import": "./dist/constants/*.js",
      "require": "./dist/constants/*.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.js"
    },
    "./validation": {
      "types": "./dist/validation/index.d.ts",
      "import": "./dist/validation/index.js",
      "require": "./dist/validation/index.js"
    },
    "./validation/*": {
      "types": "./dist/validation/*.d.ts",
      "import": "./dist/validation/*.js",
      "require": "./dist/validation/*.js"
    },
    "./domain": {
      "types": "./dist/types/domain.d.ts",
      "import": "./dist/types/domain.js",
      "require": "./dist/types/domain.js"
    },
    "./utilities": {
      "types": "./dist/types/utilities.d.ts",
      "import": "./dist/types/utilities.js",
      "require": "./dist/types/utilities.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "react-hook-form": "^7.62.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "stripe": "^18.4.0",
    "tsx": "^4.20.3"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "0.25.8",
    "@rollup/rollup-linux-x64-gnu": "4.46.2"
  }
}
